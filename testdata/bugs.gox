package testdata

import "github.com/germtb/gox"

// Bug 1: JSX comments should be stripped, not wrapped in gox.V()
func CommentsTest(props gox.Props) gox.VNode {
	return <box>
		{/* This is a comment */}
		<text>Visible</text>
		{/* Another comment */}
	</box>
}

// Bug 2: Inline map literals need type annotation
func InlineMapTest(props gox.Props) gox.VNode {
	return <box style={{
		"color":      "blue",
		"background": "white",
	}}>
		<text>Styled</text>
	</box>
}
